{
  "name": "Nanopb_Cpp",
  "version": "dev",
  "keywords": "protocol buffers, protobuf, google",
  "description": "Nanopb_Cpp is a wrapper to use Nanopb in C++ projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/nanopb/nanopb_cpp"
  },
  "authors": [{
    "name": "Pavel Sokolov",
    "email": "pavel@sokolov.me"
  }],
  "export": {
    "include": [
      "*.cpp",
      "*.h",
      "examples"
    ],
    "exclude": [
      "examples/**/CMakeLists.txt"
    ]
  },
  "srcDir": "",
  "srcFilter": [
    "+<nanopb_cpp.cpp>"
  ],
  "examples": [
    "examples/complex/complex.proto",
    "examples/complex/complex.options",
    "examples/complex/converters.hpp",
    "examples/complex/models.hpp",
    "examples/complex/main.cpp"
  ],
  "frameworks": "*",
  "platforms": "*"
}
